#include<stdio.h>
int main ()
{
float subject1,subject2,subject3,subject4,subject5,aggregate_mark,percentage_mark;
printf ("Enter marks of 1 subject= ");
scanf ("%f",&subject1);
printf ("Enter marks of 2 subject= ");
scanf ("%f",&subject2);
printf ("Enter marks of 3 subject= ");
scanf ("%f",&subject3);
printf ("Enter marks of 4 subject= ");
scanf ("%f",&subject4);
printf ("Enter marks of 5 subject ");
scanf ("%f",&subject5);
aggregate_mark = ((subject1+subject2+subject3+subject4+subject5)/5);
percentage_mark = ((subject1+subject2+subject3+subject4+subject5)/500)*100;
printf ("\nAggregate marks= %f",aggregate_mark);
printf ("\nPercentage= %f \%",percentage_mark);
}
